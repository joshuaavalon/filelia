{
  "name": "@filelia/filelia",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "start": "node ./dist/index.js | pino-pretty",
    "lint": "eslint src",
    "pretest": "npm run db:reset",
    "db:dev": "prisma migrate dev --name dev",
    "db:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force --skip-generate",
    "json:generate": "node dist/plugin/validation/json-schema/utils/print-schema.js"
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [],
  "homepage": "https://github.com/joshuaavalon/filelia#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/joshuaavalon/filelia.git"
  },
  "bugs": {
    "url": "https://github.com/joshuaavalon/filelia/issues"
  },
  "author": "Joshua Avalon",
  "license": "Apache-2.0",
  "imports": {
    "#plugin": {
      "types": "./src/plugin/index.ts",
      "import": "./dist/plugin/index.js"
    },
    "#plugin/*": {
      "types": "./src/plugin/*/index.ts",
      "import": "./dist/plugin/*/index.js"
    },
    "#plugin/validation/json-schema/utils": {
      "types": "./src/plugin/validation/json-schema/utils/index.ts",
      "import": "./dist/plugin/validation/json-schema/utils/index.js"
    },
    "#plugin/validation/json-schema/*": {
      "types": "./src/plugin/validation/json-schema/*/index.schema.ts",
      "import": "./dist/plugin/validation/json-schema/*/index.schema.js"
    },
    "#plugin/*/schema": {
      "types": "./src/plugin/*/schema.ts",
      "import": "./dist/plugin/*/schema.js"
    },
    "#provider/*": {
      "types": "./src/provider/*/index.ts",
      "import": "./dist/provider/*/index.js"
    },
    "#component/*": {
      "types": "./src/component/*/index.ts",
      "import": "./dist/component/*/index.js"
    },
    "#utils/*": {
      "types": "./src/utils/*.ts",
      "import": "./dist/utils/*.js"
    },
    "#utils": {
      "types": "./src/utils/index.ts",
      "import": "./dist/utils/index.js"
    },
    "#server": {
      "types": "./src/server/index.ts",
      "import": "./dist/server/index.js"
    },
    "#error": {
      "types": "./src/error/index.ts",
      "import": "./dist/error/index.js"
    },
    "#route": {
      "types": "./src/route/index.ts",
      "import": "./dist/route/index.js"
    },
    "#config": {
      "types": "./src/config/index.ts",
      "import": "./dist/config/index.js"
    }
  },
  "devDependencies": {
    "@garoudev/eslint-config": "^1.0.1",
    "@types/node": "^20.1.4",
    "@types/uuid": "^9.0.1",
    "dotenv": "^16.0.3",
    "prisma": "4.14.1",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@fastify/cookie": "8.3.0",
    "@fastify/helmet": "10.1.1",
    "@fastify/nextjs": "9.2.0",
    "@fastify/static": "6.10.1",
    "@fastify/type-provider-typebox": "3.2.0",
    "@prisma/client": "4.14.1",
    "@sinclair/typebox": "0.28.9",
    "ajv": "8.12.0",
    "ajv-formats": "2.1.1",
    "fastify": "4.17.0",
    "fastify-plugin": "4.5.0",
    "image-type": "5.2.0",
    "pino-pretty": "10.0.0",
    "read-env-vars": "1.0.0",
    "sharp": "0.32.1",
    "uuid": "9.0.0"
  }
}
